<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.mapper.Edu_classDao">
    
    <select id="getById" resultType="Edu_class" parameterType="int">
    select * from edu_class where id=#{id}
    </select>
    
    <select id="listAll" resultType="Edu_class">
    select * from edu_class
    </select>
  
  </mapper>